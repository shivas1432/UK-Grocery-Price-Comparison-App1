# UK Grocery Price Comparison App Environment Variables

# Server Configuration
PORT=3000
NODE_ENV=development

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Caching (Redis - optional)
REDIS_URL=redis://localhost:6379

# Monitoring
LOG_LEVEL=info

# Store API Keys (if required)
TESCO_API_KEY=your_tesco_api_key_here
SAINSBURYS_API_KEY=your_sainsburys_api_key_here

# Web Scraping Configuration
USER_AGENT="UK-Grocery-Comparison-Bot/1.0"
SCRAPING_DELAY_MS=1000
